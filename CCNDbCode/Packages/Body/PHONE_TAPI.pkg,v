text
@create or replace
PACKAGE body PHONE_tapi
IS
  -- insert
PROCEDURE ins(
    IN_PHONE_NUMBER_TYPE IN PHONE.PHONE_NUMBER_TYPE%type ,
    IN_COST_CENTER_CODE  IN PHONE.COST_CENTER_CODE%type ,
    IN_PHONE_NUMBER      IN PHONE.PHONE_NUMBER%type DEFAULT NULL )
IS
BEGIN
  INSERT
  INTO PHONE
    (
      PHONE_NUMBER_TYPE ,
      COST_CENTER_CODE ,
      PHONE_NUMBER
    )
    VALUES
    (
      IN_PHONE_NUMBER_TYPE ,
      IN_COST_CENTER_CODE ,
      IN_PHONE_NUMBER
    );
END;
-- update
PROCEDURE upd
  (
    IN_PHONE_NUMBER_TYPE IN PHONE.PHONE_NUMBER_TYPE%type ,
    IN_COST_CENTER_CODE  IN PHONE.COST_CENTER_CODE%type ,
    IN_PHONE_NUMBER      IN PHONE.PHONE_NUMBER%type DEFAULT NULL
  )
IS
BEGIN
  UPDATE PHONE
  SET PHONE_NUMBER       = IN_PHONE_NUMBER
  WHERE COST_CENTER_CODE = IN_COST_CENTER_CODE
  AND PHONE_NUMBER_TYPE  = IN_PHONE_NUMBER_TYPE;
END;
-- del
PROCEDURE del(
    IN_COST_CENTER_CODE  IN PHONE.COST_CENTER_CODE%type ,
    IN_PHONE_NUMBER_TYPE IN PHONE.PHONE_NUMBER_TYPE%type )
IS
BEGIN
  DELETE
  FROM PHONE
  WHERE COST_CENTER_CODE = IN_COST_CENTER_CODE
  AND PHONE_NUMBER_TYPE  = IN_PHONE_NUMBER_TYPE;
end;
END PHONE_tapi;@


