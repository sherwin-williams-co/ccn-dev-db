    /* Inserting data in CCN_PRICING_DISTRICT_OTHER tables for OTHER country code*/
        ELSE
            --Inserting data in CCN_PRICING_DISTRICT_OTHER tables for OTHER country code
             INSERT INTO CCN_PRICING_DISTRICT_OTHER VALUES ( cc_rec.COST_CENTER_CODE
