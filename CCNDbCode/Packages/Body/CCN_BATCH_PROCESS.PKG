        ELSIF CC_REC.COUNTRY_CODE='CAN' THEN
            --Inserting data in CCN_PRICING_DISTRICT_CAN tables for CAN country code
             INSERT INTO CCN_PRICING_DISTRICT_CAN VALUES ( cc_rec.COST_CENTER_CODE
                                                         , cc_rec.COST_CENTER_NAME
                                                         , V_ADDRESS_STREET
                                                         , V_CITY
