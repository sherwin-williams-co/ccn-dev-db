                     CASE WHEN EACH_REC.HRCHY_DTL_LEVEL = '1' THEN
                          V_HRCHY_DTL_PREV_LVL_VAL := '~~~';
                          V_HRCHY_DTL_CURR_ROW_VAL := EACH_REC.HRCHY_DTL_CURR_LVL_VAL;
