@create or replace
PACKAGE body POLLING_tapi
IS
  -- insert
PROCEDURE ins(
    IN_POLLING_IND               IN POLLING.POLLING_IND%type DEFAULT NULL ,
    IN_POLLING_SCHEDULE_CODE     IN POLLING.POLLING_SCHEDULE_CODE%type DEFAULT NULL ,
    IN_MULTICAST_IND             IN POLLING.MULTICAST_IND%type DEFAULT NULL ,
    IN_POLLING_PHONE_PFX         IN POLLING.POLLING_PHONE_PFX%type DEFAULT NULL ,
    IN_POLLING_PRIORITY          IN POLLING.POLLING_PRIORITY%type DEFAULT NULL ,
    IN_POLLING_STATUS_CODE       IN POLLING.POLLING_STATUS_CODE%type ,
    IN_TIME_ZONE                 IN POLLING.TIME_ZONE%type DEFAULT NULL ,
    IN_COST_CENTER_CODE          IN POLLING.COST_CENTER_CODE%type ,
    IN_POLLING_PHONE_NUMBER      IN POLLING.POLLING_PHONE_NUMBER%type DEFAULT NULL ,
    IN_NEXT_DOWNLOAD_BLOCK_COUNT IN POLLING.NEXT_DOWNLOAD_BLOCK_COUNT%type DEFAULT NULL )
IS
BEGIN
  INSERT
  INTO POLLING
    (
      POLLING_IND ,
      POLLING_SCHEDULE_CODE ,
      MULTICAST_IND ,
      POLLING_PHONE_PFX ,
      POLLING_PRIORITY ,
      POLLING_STATUS_CODE ,
      TIME_ZONE ,
      COST_CENTER_CODE ,
      POLLING_PHONE_NUMBER ,
      NEXT_DOWNLOAD_BLOCK_COUNT
    )
    VALUES
    (
      IN_POLLING_IND ,
      IN_POLLING_SCHEDULE_CODE ,
      IN_MULTICAST_IND ,
      IN_POLLING_PHONE_PFX ,
      IN_POLLING_PRIORITY ,
      IN_POLLING_STATUS_CODE ,
      IN_TIME_ZONE ,
      IN_COST_CENTER_CODE ,
      IN_POLLING_PHONE_NUMBER ,
      IN_NEXT_DOWNLOAD_BLOCK_COUNT
    );
END;
-- update
PROCEDURE upd
  (
    IN_POLLING_IND               IN POLLING.POLLING_IND%type DEFAULT NULL ,
    IN_POLLING_SCHEDULE_CODE     IN POLLING.POLLING_SCHEDULE_CODE%type DEFAULT NULL ,
    IN_MULTICAST_IND             IN POLLING.MULTICAST_IND%type DEFAULT NULL ,
    IN_POLLING_PHONE_PFX         IN POLLING.POLLING_PHONE_PFX%type DEFAULT NULL ,
    IN_POLLING_PRIORITY          IN POLLING.POLLING_PRIORITY%type DEFAULT NULL ,
    IN_POLLING_STATUS_CODE       IN POLLING.POLLING_STATUS_CODE%type ,
    IN_TIME_ZONE                 IN POLLING.TIME_ZONE%type DEFAULT NULL ,
    IN_COST_CENTER_CODE          IN POLLING.COST_CENTER_CODE%type ,
    IN_POLLING_PHONE_NUMBER      IN POLLING.POLLING_PHONE_NUMBER%type DEFAULT NULL ,
    IN_NEXT_DOWNLOAD_BLOCK_COUNT IN POLLING.NEXT_DOWNLOAD_BLOCK_COUNT%type DEFAULT NULL
  )
IS
BEGIN
  UPDATE POLLING
  SET POLLING_IND             = IN_POLLING_IND ,
    POLLING_SCHEDULE_CODE     = IN_POLLING_SCHEDULE_CODE ,
    MULTICAST_IND             = IN_MULTICAST_IND ,
    POLLING_PHONE_PFX         = IN_POLLING_PHONE_PFX ,
    POLLING_PRIORITY          = IN_POLLING_PRIORITY ,
    TIME_ZONE                 = IN_TIME_ZONE ,
    POLLING_PHONE_NUMBER      = IN_POLLING_PHONE_NUMBER ,
    NEXT_DOWNLOAD_BLOCK_COUNT = IN_NEXT_DOWNLOAD_BLOCK_COUNT
  WHERE COST_CENTER_CODE      = IN_COST_CENTER_CODE
  AND POLLING_STATUS_CODE     = IN_POLLING_STATUS_CODE;
END;
-- del
PROCEDURE del(
    IN_COST_CENTER_CODE    IN POLLING.COST_CENTER_CODE%type ,
    IN_POLLING_STATUS_CODE IN POLLING.POLLING_STATUS_CODE%type )
IS
BEGIN
  DELETE
  FROM POLLING
  WHERE COST_CENTER_CODE  = IN_COST_CENTER_CODE
  AND POLLING_STATUS_CODE = IN_POLLING_STATUS_CODE;
end;
END POLLING_tapi;@


